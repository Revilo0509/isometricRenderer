{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.o": true,
		"**/*.exe": true,
	},
	"files.associations": {
		"*.md": "markdown",
		"xstring": "cpp",
		"*.m": "cpp",
		"xiosbase": "cpp",
		"vector": "cpp",
		"iostream": "cpp"
	}
}